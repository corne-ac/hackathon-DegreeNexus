{
  "name": "pixel-pioneers-hackathon",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npx prisma generate && vite dev",
    "build": "npx prisma generate --no-engine && vite build",
    "preview": "npx prisma generate && vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "wrangler": "npx wrangler pages dev -- npm run dev\n"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "2.3.0",
    "@skeletonlabs/tw-plugin": "0.2.2",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "10.4.16",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "prisma": "^5.4.2",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "3.3.3",
    "tslib": "^2.4.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.2",
    "vite-plugin-tailwind-purgecss": "0.1.3"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "1.5.3",
    "@prisma/client": "^5.4.2",
    "@prisma/extension-accelerate": "^0.6.2",
    "@sentry/node": "^7.74.1",
    "highlight.js": "^11.9.0",
    "wrangler": "^3.14.0"
  }
}